nter\">\n              <v-btn\n                color=\"primary\"\n                rounded=\"pill\"\n                @click=\"changeTab('tab-2')\"\n                v-if=\"store.cart.length >= 1\"\n                >CheckOut</v-btn\n              >\n            </v-col>\n          </v-row>\n        </v-window-item>\n        <v-window-item value=\"tab-2\" class=\"pa-1\">\n          <StepSecond />\n          <v-row class=\"mt-3\">\n            <v-col cols=\"6\">\n              <v-btn color=\"primary\" rounded=\"pill\" variant=\"tonal\" @click=\"changeTab('tab-1')\"\n                >Back</v-btn\n              >\n            </v-col>\n            <v-col cols=\"6\" class=\"text-right pb-5\">\n              <v-btn color=\"primary\" rounded=\"pill\" @click=\"changeTab('tab-3')\"\n                >Place an Order</v-btn\n              >\n            </v-col>\n          </v-row>\n        </v-window-item>\n        <v-window-item value=\"tab-3\" class=\"pa-1\">\n          <Payment />\n          <v-row class=\"mt-3\">\n            <v-col cols=\"12\" sm=\"6\">\n              <v-btn color=\"primary\" variant=\"tonal\" rounded=\"pill\" @click=\"changeTab('tab-2')\"\n                >Back</v-btn\n              >\n            </v-col>\n            <v-col cols=\"12\" sm=\"6\" class=\"text-sm-right pb-5\">\n              <v-btn color=\"primary\" rounded=\"pill\" @click=\"thankyou = true\"\n                >Complete an Order</v-btn\n              >\n              <!-- Modal -->\n              <v-dialog v-model=\"thankyou\" max-width=\"750\">\n                <Thankyou />\n              </v-dialog>\n            </v-col>\n          </v-row>\n        </v-window-item>\n      </v-window>\n    </v-card-text>\n  </v-card>\n</template>\n<style lang=\"scss\">\n.customTab {\n  min-height: 68px;\n}\n</style>\n"],"version":3}