 'Taxable Goods', 'Downloadable Product']);\nconst slider = ref(40);\n</script>\n<template>\n  <div class=\"pa-1 pt-0\">\n    <!-- General -->\n    <v-card elevation=\"10\" class=\"mb-6\">\n      <v-card-text>\n        <h5 class=\"text-h5 mb-8\">General</h5>\n        <v-row class=\"d-flex flex-cols gap-3\">\n          <v-col cols=\"12\">\n            <v-label class=\"font-weight-medium mb-2\"\n              >Product Name <span class=\"text-error ms-1\">*</span></v-label\n            >\n            <VTextField\n              type=\"text\"\n              value=\"Sample product\"\n              placeholder=\"Product Name\"\n              variant=\"outlined\"\n              hide-details\n            ></VTextField>\n            <p class=\"textSecondary text-12 mt-1\">\n              A product name is required and recommended to be unique.\n            </p>\n          </v-col>\n          <v-col cols=\"12\">\n            <v-label class=\"font-weight-medium mb-2\">Description</v-label>\n            <v-card variant=\"outlined\">\n              <div v-if=\"editor\">\n                <EditorMenubar :editor=\"editor\" class=\"border-b\" />\n              </div>\n              <editor-content :editor=\"editor\" style=\"min-height: 150px\" />\n            </v-card>\n            <p class=\"textSecondary text-12 mt-1\">\n              Set a description to the product for better visibility.\n            </p>\n          </v-col>\n        </v-row>\n      </v-card-text>\n    </v-card>\n\n    <!-- Media -->\n    <v-card elevation=\"10\" class=\"mb-6\">\n      <v-card-text>\n        <h5 class=\"text-h5 mb-8\">Media</h5>\n        <v-file-input\n          color=\"primary\"\n          label=\"Drop files here or click to upload.\"\n          placeholder=\"Select your files\"\n          prepend-icon=\"mdi-paperclip\"\n          variant=\"outlined\"\n          chips\n          multiple\n        ></v-file-input>\n      </v-card-text>\n    </v-card>\n\n    <!-- Pricing -->\n    <v-card elevation=\"10\" class=\"mb-6\">\n      <v-card-text>\n        <h5 class=\"text-h5 mb-8\">Pricing</h5>\n        <v-row class=\"d-flex flex-cols gap-3\">\n          <v-col cols=\"12\">\n            <v-label class=\"font-weight-medium mb-2\"\n              >Base Price <span class=\"text-error ms-1\">*</span></v-label\n            >\n            <VTextField\n              type=\"text\"\n              value=\"199.99\"\n              placeholder=\"Product price\"\n              variant=\"outlined\"\n              hide-details\n            ></VTextField>\n            <p class=\"textSecondary text-12 mt-1\">Set the product price.</p>\n          </v-col>\n          <v-col cols=\"12\">\n            <div class=\"d-flex align-center\">\n              <v-label class=\"font-weight-medium mb-2 me-1\">Discount Type </v-label>\n              <v-menu open-on-hover location=\"top\">\n                <template v-slot:activator=\"{ props }\">\n                  <RouterLink to=\"#\" v-bind=\"props\" class=\"textSecondary\" icon>\n                    <InfoCircleIcon height=\"15\" />\n                  </RouterLink>\n                </template>\n                <v-sheet rounded=\"sm\" width=\"180\" class=\"pa-4 text-center text-body-2\" elevation-10>\n                  Select a discount type that will be applied to this product\n                </v-sheet>\n              </v-menu>\n            </div>\n\n            <v-radio-group v-model=\"discount\">\n              <div class=\"d-sm-flex d-block gap-3 justify-space-between\">\n                <div class=\"py-3 px-3 w-100 border border-dashed rounded-md mb-6\">\n                  <v-radio value=\"no-discount\" color=\"primary\">\n                    <template v-slot:label>\n                      <div class=\"d-flex align-center w-100\">\n                        <h6 class=\"text-h6\">No Discount</h6>\n                      </div>\n                    </template>\n                  </v-radio>\n                </div>\n\n                <div class=\"py-3 px-3 w-100 border border-dashed rounded-md mb-6\">\n                  <v-radio value=\"percent\" color=\"primary\">\n                    <template v-slot:label>\n                      <h6 class=\"text-h6\">Percentage %</h6>\n                    </template>\n                  </v-radio>\n                </div>\n\n                <div class=\"py-3 px-3 w-100 border border-dashed rounded-md mb-6\">\n                  <v-radio value=\"fixed\" color=\"primary\">\n                    <template v-slot:label>\n                      <h6 class=\"text-h6\">Fixed Price</h6>\n                    </template>\n                  </v-radio>\n                </div>\n              </div>\n            </v-radio-group>\n\n            <!-- if percent is selected -->\n            <div v-if=\"discount == 'no-discount'\">\n              <v-row>\n                <v-col cols=\"12\" md=\"6\" class=\"pt-0\">\n                  <v-label class=\"font-weight-medium mb-2\"\n                    >Tax Class <span class=\"text-error ms-1\">*</span></v-label\n                  >\n                  <v-select\n                    v-model=\"select\"\n                    :items=\"items\"\n                    variant=\"outlined\"\n                    density=\"compact\"\n                    class=\"text-body-1\"\n                    hide-details\n                  ></v-select>\n                  <p class=\"textSecondary text-12 mt-1\">Set the product tax class.</p>\n                </v-col>\n                <v-col cols=\"12\" md=\"6\" class=\"pt-0\">\n                  <v-label class=\"font-weight-medium mb-2\"\n                    >VAT Amount (%) <span class=\"text-error ms-1\">*</span></v-label\n                  >\n                  <VTextField\n                    type=\"text\"\n                    placeholder=\"Product Name\"\n                    variant=\"outlined\"\n                    hide-details\n                  ></VTextField>\n                  <p class=\"textSecondary text-12 mt-1\">Set the product VAT about.</p>\n                </v-col>\n              </v-row>\n            </div>\n            <div v-if=\"discount == 'percent'\">\n              <v-row>\n                <v-col cols=\"12\">\n                  <v-label class=\"font-weight-medium mb-2\"\n                    >Set Discount Percentage <span class=\"text-error ms-1\">*</span></v-label\n                  >\n                  <h3 class=\"text-h2 text-center\">{{ slider }}</h3>\n                  <v-slider\n                    class=\"mt-3\"\n                    v-model=\"slider\"\n                    hide-details\n                    color=\"primary\"\n                    step=\"10\"\n                    show-ticks=\"always\"\n                    thumb-label=\"always\"\n                  ></v-slider>\n                  <p class=\"textSecondary text-12 mt-1\">\n                    Set a percentage discount to be applied on this product.\n                  </p>\n                </v-col>\n              </v-row>\n              <v-row class=\"mt-6\">\n                <v-col cols=\"12\" md=\"6\" class=\"pt-0\">\n                  <v-label class=\"font-weight-medium mb-2\"\n                    >Tax Class <span class=\"text-error ms-1\">*</span></v-label\n                  >\n                  <v-select\n                    v-model=\"select\"\n                    :items=\"items\"\n                    variant=\"outlined\"\n                    density=\"compact\"\n                    class=\"text-body-1\"\n                    hide-details\n                  ></v-select>\n                  <p class=\"textSecondary text-12 mt-1\">Set the product tax class.</p>\n                </v-col>\n                <v-col cols=\"12\" md=\"6\" class=\"pt-0\">\n                  <v-label class=\"font-weight-medium mb-2\"\n                    >VAT Amountb (%) <span class=\"text-error ms-1\">*</span></v-label\n                  >\n                  <VTextField\n                    type=\"text\"\n                    value=\"35\"\n                    placeholder=\"\"\n                    variant=\"outlined\"\n                    hide-details\n                  ></VTextField>\n                  <p class=\"textSecondary text-12 mt-1\">Set the product VAT about.</p>\n                </v-col>\n              </v-row>\n            </div>\n            <div v-if=\"discount == 'fixed'\">\n              <v-row>\n                <v-col cols=\"12\" class=\"pt-0 mb-3\">\n                  <v-label class=\"font-weight-medium mb-2\">Fixed Discounted Price</v-label>\n                  <VTextField\n                    type=\"text\"\n                    placeholder=\"Discounted Price\"\n                    variant=\"outlined\"\n                    hide-details\n                  ></VTextField>\n                  <p class=\"textSecondary text-12 mt-1\">\n                    Set the discounted product price. The product will be reduced at the determined\n                    fixed price\n                  </p>\n                </v-col>\n                <v-col cols=\"12\" md=\"6\" class=\"pt-0\">\n                  <v-label class=\"font-weight-medium mb-2\"\n                    >Tax Class <span class=\"text-error ms-1\">*</span></v-label\n                  >\n                  <v-select\n                    v-model=\"select\"\n                    :items=\"items\"\n                    variant=\"outlined\"\n                    density=\"compact\"\n                    class=\"text-body-1\"\n                    hide-details\n                  ></v-select>\n                  <p class=\"textSecondary text-12 mt-1\">Set the product tax class.</p>\n                </v-col>\n                <v-col cols=\"12\" md=\"6\" class=\"pt-0\">\n                  <v-label class=\"font-weight-medium mb-2\"\n                    >VAT Amount (%) <span class=\"text-error ms-1\">*</span></v-label\n                  >\n                  <VTextField\n                    type=\"text\"\n                    placeholder=\"\"\n                    variant=\"outlined\"\n                    hide-details\n                  ></VTextField>\n                  <p class=\"textSecondary text-12 mt-1\">Set the product VAT about.</p>\n                </v-col>\n              </v-row>\n            </div>\n          </v-col>\n        </v-row>\n      </v-card-text>\n    </v-card>\n  </div>\n  <div class=\"d-flex mb-md-0 mb-3 gap-3\">\n    <v-btn flat color=\"primary\"> save changes </v-btn>\n    <v-btn variant=\"tonal\" color=\"error\"> cancel </v-btn>\n  </div>\n</template>\n","<script setup lang=\"ts\">\nimport { XIcon } from 'vue-tabler-icons';\nimport { ref } from 'vue';\nimport { useEditor, EditorContent } from '@tiptap/vue-3';\nimport EditorMenubar from '@/components/forms/plugins/editor/EditorMenubar.vue';\nimport StarterKit from '@tiptap/starter-kit';\nconst editor = useEditor({\n  extensions: [StarterKit],\n});\nconst checkbox = ref(false);\nconst shipping = ref(false);\nconst ship = ref('ship');\nconst select = ref('Size');\nconst items = ref(['Color', 'Size', 'Material', 'Style']);\n</script>\n<template>\n  <div class=\"pa-1 pt-0\">\n    <!-- Inventory -->\n    <v-card elevation=\"10\" class=\"mb-6\">\n      <v-card-text>\n        <h5 class=\"text-h5 mb-8\">Inventory</h5>\n        <v-row>\n          <v-col cols=\"12\">\n            <v-label class=\"font-weight-medium mb-2\"\n              >SKU <span class=\"text-error ms-1\">*</span></v-label\n            >\n            <VTextField\n              type=\"text\"\n              value=\"011985001\"\n              placeholder=\"SKU Number\"\n              variant=\"outlined\"\n              hide-details\n            ></VTextField>\n            <p class=\"textSecondary text-12 mt-1\">Enter the product SKU.</p>\n          </v-col>\n          <v-col cols=\"12\">\n            <v-label class=\"font-weight-medium mb-2\"\n              >Barcode <span class=\"text-error ms-1\">*</span></v-label\n            >\n            <VTextField\n              type=\"text\"\n              value=\"45874521458\"\n              placeholder=\"Barcode Number\"\n              variant=\"outlined\"\n              hide-details\n            ></VTextField>\n            <p class=\"textSecondary text-12 mt-1\">Enter the product barcode number.</p>\n          </v-col>\n          <v-col cols=\"12\" md=\"6\">\n            <v-label class=\"font-weight-medium mb-2\"\n              >Quantity <span class=\"text-error ms-1\">*</span></v-label\n            >\n            <VTextField type=\"number\" value=\"24\" variant=\"outlined\" hide-details></VTextField>\n            <p class=\"textSecondary text-12 mt-1\">Enter the product quantity.</p>\n          </v-col>\n          <v-col cols=\"12\" md=\"6\">\n            <v-label class=\"font-weight-medium mb-3\"><span class=\"text-error ms-1\"></span></v-label>\n            <VTextField\n              type=\"number\"\n              placeholder=\"In warehouse\"\n              variant=\"outlined\"\n              hide-details\n            ></VTextField>\n          </v-col>\n          <v-col cols=\"12\">\n            <v-label class=\"font-weight-medium mb-2\">Allow Backorders</v-label>\n            <div class=\"d-flex flex-wrap align-center ml-n2\">\n              <v-checkbox class=\"pe-2\" v-model=\"checkbox\" required hide-details color=\"primary\">\n                <template v-slot:label class=\"font-weight-medium\">Yes</template>\n              </v-checkbox>\n            </div>\n            <p class=\"textSecondary text-12 mt-n2\">\n              Allow customers to purchase products that are out of stock.\n            </p>\n          </v-col>\n        </v-row>\n      </v-card-text>\n    </v-card>\n\n    <!-- Variations -->\n    <v-card elevation=\"10\" class=\"mb-6\">\n      <v-card-text>\n        <h5 class=\"text-h5 mb-8\">Variations</h5>\n\n        <v-row>\n          <v-col cols=\"12\" md=\"10\">\n            <v-label class=\"font-weight-medium mb-2\">Add Product Variations</v-label>\n            <v-row>\n            