     <div v-if=\"ship == 'ship'\" class=\"mt-8\">\n          <v-row>\n            <v-col cols=\"12\" class=\"pt-0 mb-3\">\n              <v-label class=\"font-weight-medium mb-2\">Weight</v-label>\n              <VTextField\n                type=\"text\"\n                value=\"4.5\"\n                placeholder=\"Product weight\"\n                variant=\"outlined\"\n                hide-details\n              ></VTextField>\n              <p class=\"textSecondary text-12 mt-1\">Set a product weight in kilograms (kg).</p>\n            </v-col>\n            <v-col cols=\"12\">\n              <v-label class=\"font-weight-medium mb-2\">Dimension</v-label>\n              <div class=\"d-md-flex gap-3\">\n                <VTextField\n                  type=\"number\"\n                  value=\"15\"\n                  placeholder=\"Width (w)\"\n                  variant=\"outlined\"\n                  class=\"mb-md-0 mb-3\"\n                  hide-details\n                ></VTextField>\n                <VTextField\n                  type=\"number\"\n                  value=\"12\"\n                  placeholder=\"Height (h)\"\n                  variant=\"outlined\"\n                  class=\"mb-md-0 mb-3\"\n                  hide-details\n                ></VTextField>\n                <VTextField\n                  type=\"number\"\n                  value=\"18\"\n                  placeholder=\"Lengtn (l)\"\n                  variant=\"outlined\"\n                  class=\"mb-md-0 mb-3\"\n                  hide-details\n                ></VTextField>\n              </div>\n              <p class=\"textSecondary text-12 mt-1\">\n                Enter the product dimensions in centimeters (cm).\n              </p>\n            </v-col>\n          </v-row>\n        </div>\n      </v-card-text>\n    </v-card>\n\n    <!-- Meta Options -->\n    <v-card elevation=\"10\" class=\"mb-6\">\n      <v-card-text>\n        <h5 class=\"text-h5 mb-4\">Meta Options</h5>\n        <v-row>