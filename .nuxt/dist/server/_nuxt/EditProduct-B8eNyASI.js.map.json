      </div>\n          <div class=\"mt-8\">\n            <v-label class=\"font-weight-medium mb-2\">Tags</v-label>\n            <v-combobox\n              variant=\"outlined\"\n              density=\"comfortable\"\n              color=\"primary\"\n              v-model=\"tagsselect\"\n              :items=\"tagsitems\"\n              chips\n              multiple\n            ></v-combobox>\n            <p class=\"text-12 textSecondary mt-n4\">Add tags to a product.</p>\n          </div>\n        </v-card-item>\n      </v-card>\n\n      <!-- Daily Sales -->\n      <DailySalesChart />\n\n      <!-- Product Template -->\n      <v-card elevation=\"10\" class=\"mb-6\">\n        <v-card-item>\n          <h5 class=\"text-h5 mb-5\">Product Template</h5>\n          <v-label class=\"font-weight-medium mb-2\">Select a product template</v-label>\n          <v-select\n            v-model=\"templateselect\"\n            :items=\"templateitems\"\n            variant=\"outlined\"\n            class=\"text-body-1\"\n            placeholder=\"Default template\"\n            hide-details\n          ></v-select>\n          <p class=\"text-12 textSecondary mt-1\">\n            Assign a template from your current admin to define how a single product is displayed.\n          </p>\n        </v-card-item>\n      </v-card>\n    </div>\n  </div>\n</template>\n","<script setup>\nimport { ref } from 'vue';\nimport BaseBreadcrumb from '@/components/shared/BaseBreadcrumb.vue';\nimport LeftSide from '@/components/apps/ecommerce/editproduct/LeftSide.vue';\nimport RightSide from '@/components/apps/ecommerce/editproduct/RightSide.vue';\n\nconst page = ref({ title: 'Edit Product' });\nconst breadcrumbs = ref([\n  {\n    text: 'Dashboard',\n    disabled: false,\n    href: '/',\n  },\n  {\n    text: 'Edit Product',\n    disabled: true,\n    href: '#',\n  },\n]);\n</script>\n<template>\n  <BaseBreadcrumb :title=\"page.title\" :breadcrumbs=\"breadcrumbs\"></BaseBreadcrumb>\n  <v-row>\n    <v-col cols=\