xt\"\n                    v-model=\"some_imp1\"\n                    persistent-hint\n                    variant=\"outlined\"\n                    color=\"primary\"\n                  ></v-text-field>\n                </v-col>\n\n                <v-col cols=\"12\" lg=\"4\">\n                  <v-text-field\n                    label=\"Dense\"\n                    hint=\"Some important text\"\n                    v-model=\"some_imp2\"\n                    persistent-hint\n                    variant=\"outlined\"\n                    color=\"primary\"\n                    density=\"compact\"\n                  ></v-text-field>\n                </v-col>\n\n                <v-col cols=\"12\" lg=\"4\">\n                  <v-text-field\n                    label=\"Normal\"\n                    hint=\"Some important text\"\n                    v-model=\"some_imp3\"\n                    persistent-hint\n                    variant=\"outlined\"\n                    color=\"primary\"\n                  ></v-text-field>\n                </v-col>\n              </v-row>\n            </UiChildCard>\n          </v-col>\n        </v-row>\n      </UiParentCard>\n    </v-col>\n  </v-row>\n</template>\n"],"version":3}