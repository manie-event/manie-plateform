e,\n      borderColor: 'rgba(0,0,0,0.1)',\n      strokeDashArray: 1,\n      xaxis: {\n        lines: {\n          show: false,\n        },\n      },\n      yaxis: {\n        lines: {\n          show: true,\n        },\n      },\n      padding: {\n        top: 0,\n        right: 0,\n        bottom: 0,\n        left: 0,\n      },\n    },\n    colors: [getPrimary.value, '#D9D9D9'],\n    plotOptions: {\n      bar: {\n        horizontal: false,\n        // barHeight: \"90%\",\n        columnWidth: '26%',\n        borderRadius: [3],\n        borderRadiusApplication: 'end',\n        borderRadiusWhenStacked: 'all',\n      },\n    },\n    dataLabels: {\n      enabled: false,\n    },\n\n    xaxis: {\n      categories: [['M'], ['T'], ['W'], ['T'], ['F'], ['S'], ['S']],\n      axisBorder: {\n        show: false,\n      },\n      axisTicks: {\n        show: false,\n      },\n      //For Change Text Color\n      labels: {\n        style: {\n          colors: getTextGrey100.value,\n        },\n      },\n    },\n    yaxis: {\n      labels: {\n        show: false,\n      },\n    },\n    tooltip: {\n      theme: 'dark',\n    },\n    legend: {\n      show: false,\n    },\n  };\n});\n</script>\n<template>\n  <v-card elevation=\"10\" class=\"roun-\">\n    <v-card-text class=\"position-relative\">\n      <div class=\"d-flex justify-space-between d-block align-center\">\n        <div>\n          <h5 class=\"text-h5 mb-1 font-weight-semibold\">Payments</h5>\n          <div class=\"text-subtitle-1 text-grey100 pb-1 font-weight-medium\">Last 7 days</div>\n        </div>\n        <div class=\"text-right\">\n          <h4 class=\"text-h5 mb-1 font-weight-semibold\">12,389</h4>\n          <v-chip color=\"warning\" class=\"bg-lightwarning\" variant=\"outlined\" size=\"x-small\"\n            >-3.8%</v-chip\n          >\n        </div>\n      </div>\n      <div class=\"mb-4\">\n        <ClientOnly\n          ><apexchart\n            type=\"bar\"\n            class=\"paymentchart\"\n            hei