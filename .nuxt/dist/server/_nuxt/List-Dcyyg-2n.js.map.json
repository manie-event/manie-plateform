i-components/list/CustomAlignList.vue","../../../../components/ui-components/list/ScrollableList.vue","../../../../pages/ui-components/List.vue"],"sourcesContent":["<script setup lang=\"ts\">\nimport { shallowRef } from 'vue';\nimport {\n  AppsIcon,\n  CircleDotIcon,\n  DragDropIcon,\n  FolderIcon,\n  ChevronUpIcon,\n  ChevronDownIcon,\n} from 'vue-tabler-icons';\nconst props = defineProps(['icon']);\nconst component = props.icon;\n// List 1,3 Data\nconst list1 = shallowRef([\n  {\n    name: 'Sample Page',\n    icon: AppsIcon,\n    id: 1,\n    link: '/basic/list',\n  },\n  {\n    name: 'Elements',\n    icon: CircleDotIcon,\n    id: 2,\n    link: '',\n  },\n  {\n    name: 'Page Layouts',\n    icon: FolderIcon,\n    id: 3,\n    link: '',\n  },\n  {\n    name: 'Components',\n    icon: DragDropIcon,\n    id: 4,\n    link: '',\n  },\n]);\n</script>\n<template>\n  <v-list>\n    <v-card variant=\"outlined\" class=\"py-2\">\n      <v-list-item\n        class=\"mb-2\"\n        v-for=\"(list, i) in list1\"\n        :value=\"list\"\n        rounded=\"md\"\n        :key=\"i\"\n        color=\"primary\"\n      >\n        <template v-slot:prepend>\n          <component :is=\"list.icon\" size=\"20\" stroke-width=\"1.5\" class=\"mr-2\" />\n        </template>\n        <v-list-item-title v-text=\"list.name\"></v-list-item-title>\n      </v-list-item>\n    </v-card>\n  </v-list>\n</template>\n","<script setup lang=\"ts\">\nimport { ref } from 'vue';\nimport { CircleDotIcon, DragDropIcon, InboxIcon, MailIcon, SendIcon } from 'vue-tabler-icons';\nconst props = defineProps(['icon']);\n// List 2 data\nconst open = ref(['Users']);\n</script>\n<template>\n  <v-list>\n    <v-card variant=\"outlined\" class=\"py-2\">\n      <div class=\"text-subttile-2 pl-4 py-3\">Nested List Items</div>\n      <v-list-item value=\"\" class=\"mb-2\" color=\"primary\">\n        <template v-slot:prepend>\n          <SendIcon size=\"20\" stroke-width=\"1.5\" class=\"mr-2\" />\n        </template>\n        <v-list-item-title>Sent Mail</v-list-item-title>\n      </v-list-item>\n      <v-list-item value=\"\" class=\"mb-2\" color=\"primary\">\n        <template v-slot:prepend>\n          <MailIcon size=\"20\" stroke-width=\"1.5\" class=\"mr-2\" />\n        </template>\n        <v-list-item-title>Drafts</v-list-item-title>\n      </v-list-item>\n      <v-list-group value=\"Users\">\n        <template v-slot:activator=\"{ props }\">\n          <v-list-item v-bind=\"props\" rounded=\"md\" color=\"primary\" title=\"inbox\">\n            <template v-slot:prepend>\n              <InboxIcon size=\"20\" stroke-width=\"1.5\" class=\"mr-2\" />\n            </template>\n          </v-list-item>\n        </template>\n        <v-list-item value=\"Started\" color=\"primary\" title=\"Started\" rounded=\"md\">\n          <template v-slot:prepend>\n            <CircleDotIcon size=\"15\" stroke-width=\"1.5\" class=\"mr-2\" />\n          </template>\n        </v-list-item>\n      </v-list-group>\n    </v-card>\n  </v-list>\n</template>\n","<script setup lang=\"ts\">\nimport { shallowRef } from 'vue';\nimport { FolderIcon, BriefcaseIcon, BeachIcon } from 'vue-tabler-icons';\nconst props = defineProps(['icon']);\nconst folders = shallowRef([\n  {\n    subtitle: 'Jan 9, 2022',\n    title: 'Photos',\n    icon: FolderIcon,\n  },\n  {\n    subtitle: 'Jan 17, 2022',\n    title: 'Works',\n    icon: BriefcaseIcon,\n  },\n  {\n    subtitle: 'Jan 18, 2022',\n    title: 'Vacation',\n    icon: BeachIcon,\n  },\n]);\n</script>\n<template>\n  <v-list lines=\"two\">\n    <v-card variant=\"outlined\">\n      <v-list-item v-for=\"folder in folders\" :key=\"folder.title\" :subtitle=\"folder.subtitle\">\n        <template v-slot:prepend>\n          <v-avatar color=\"grey400\">\n            <component :is=\"folder.icon\" size=\"20\" stroke-width=\"1.5\" />\n          </v-avatar>\n        </template>\n\n        <template v-slot:title>\n          <h5 class=\"text-subtitle-1\">{{ folder.title }}</h5>\n        </template>\n        <template v-slot:subtitle>\n          <span class=\"text-subtitle-2\">{{ folder.subtitle }}</span>\n        </template>\n      </v-list-item>\n    </v-card>\n  </v-list>\n</template>\n","<script setup lang=\"ts\">\nimport { shallowRef } from 'vue';\nimport {\n  AppsIcon,\n  CircleDotIcon,\n  DragDropIcon,\n  FolderIcon,\n  ChevronUpIcon,\n  ChevronDownIcon,\n} from 'vue-tabler-icons';\nconst props = defineProps(['icon']);\nconst component = props.icon;\n// List 1,3 Data\nconst list1 = shallowRef([\n  {\n    name: 'Sample Page',\n    icon: AppsIcon,\n    id: 1,\n    link: '/basic/list',\n  },\n  {\n    name: 'Elements',\n    icon: CircleDotIcon,\n    id: 2,\n    link: '',\n  },\n  {\n    name: 'Page Layouts',\n    icon: FolderIcon,\n    id: 3,\n    link: '',\n  },\n]);\n</script>\n<template>\n  <v-list disabled>\n    <v-card variant=\"outlined\" class=\"py-2\">\n      <v-list-item\n        class=\"mb-2\"\n        v-for=\"(list, i) in list1\"\n        :value=\"list\"\n        rounded=\"md\"\n        :key=\"i\"\n        color=\"primary\"\n      >\n        <template v-slot:prepend>\n          <component :is=\"list.icon\" size=\"20\" stroke-width=\"1.5\" class=\"mr-2\" />\n        </template>\n        <v-list-item-title v-text=\"list.name\"></v-list-item-title>\n      </v-list-item>\n    </v-card>\n  </v-list>\n</template>\n","<script setup lang=\"ts\">\nimport { ref } from 'vue';\nimport { WifiIcon, BluetoothIcon } from 'vue-tabler-icons';\n// List 4 data\nconst swt1 = ref(true);\nconst swt2 = ref(false);\n</script>\n<template>\n  <v-list>\n    <v-card variant=\"outlined\" class=\"py-2\">\n      <v-list-item value=\"wifi\" class=\"mb-2\" color=\"primary\">\n        <div class=\"d-flex align-center\">\n          <WifiIcon size=\"22\" stroke-width=\"1.5\" class=\"mr-2\" />\n          <div class=\"ml-3\">Wi-Fi</div>\n          <div class=\"ml-auto\">\n            <v-switch v-model=\"swt1\" inset color=\"primary\" hide-details></v-switch>\n          </div>\n        </div>\n      </v-list-item>\n      <v-list-item value=\"bluetooth\" class=\"mb-2\" color=\"primary\">\n        <div class=\"d-flex align-center\">\n          <BluetoothIcon size=\"22\" stroke-width=\"1.5\" class=\"mr-2\" />\n          <div class=\"ml-3\">Bluetooth</div>\n          <div class=\"ml-auto\">\n            <v-switch v-model=\"swt2\" inset color=\"primary\" hide-details></v-switch>\n          </div>\n        </div>\n      </v-list-item>\n    </v-card>\n  </v-list>\n\n  <!-- <div class=\"rounded-sm px-5 py-3 my-3\">\n        <div class=\"d-flex align-center\">\n            <WifiIcon size=\"20\" stroke-width=\"1.5\"/>\n\n        </div>\n        <v-switch v-model=\"swt1\" inset label=\"Start DND Mode\" color=\"primary\" hide-details></v-switch>\n        <v-switch v-model=\"swt2\" inset  label=\"Allow Notifications\" color=\"primary\" hide-details> </v-switch>\n    </div> -->\n</template>\n","<script setup lang=\"ts\">\nimport { shallowRef } from 'vue';\nimport { AppsIcon } from 'vue-tabler-icons';\nconst props = defineProps(['icon']);\nconst component = props.icon;\n// List 1,3 Data\nconst list1 = shallowRef([\n  {\n    name: 'Sample Page',\n    icon: AppsIcon,\n    id: 1,\n    link: '/',\n  },\n  {\n    name: 'Elements',\n    icon: AppsIcon,\n    id: 2,\n    link: '',\n  },\n  {\n    name: 'Page Layouts',\n    icon: AppsIcon,\n    id: 3,\n    link: '',\n  },\n]);\n</script>\n<template>\n  <v-list>\n    <v-list-item\n      class=\"mb-1\"\n      v-for=\"(list, i) in list1\"\n      :value=\"list\"\n      rounded=\"xl\"\n      :key=\"i\"\n      color=\"primary\"\n    >\n      <template v-slot:prepend>\n        <component :is=\"list.icon\" size=\"20\" stroke-width=\"1.5\" class=\"mr-2\" />\n      </template>\n      <v-list-item-title v-text=\"list.name\"></v-list-item-title>\n    </v-list-item>\n  </v-list>\n</template>\n","<script setup lang=\"ts\">\nimport { shallowRef } from 'vue';\nimport { ChevronUpIcon } from 'vue-tabler-icons';\nimport { ChevronDownIcon } from 'vue-tabler-icons';\nconst props = defineProps(['icon']);\nconst component = props.icon;\n// custom list data\nconst customs = shallowRef([\n  {\n    title: 'Bajaj Finsery',\n    profit: true,\n    percent: '10',\n    price: '1839.00',\n  },\n  {\n    title: 'TTML',\n    profit: false,\n    percent: '10',\n    price: '100.00',\n  },\n  {\n    title: 'Reliance',\n    profit: true,\n    percent: '10',\n    price: '200.00',\n  },\n  {\n    title: 'TTML',\n    profit: false,\n    percent: '10',\n    price: '189.00',\n  },\n]);\n</script>\n<template>\n  <v-list two-lines>\n    <template v-for=\"(custom, i) in customs\" :key=\"i\">\n      <v-list-item color=\"primary\" class=\"py-3\">\n        <v-list-item-title class=\"text-subtitle-1\">{{ custom.title }}</v-list-item-title>\n        <v-list-item-subtitle\n          v-if=\"custom.profit\"\n          class=\"text-subtitle-2 text-success text-high-emphasis\"\n          >{{ custom.percent }}% Profit</v-list-item-subtitle\n        >\n        <v-list-item-subtitle v-else class=\"text-subtitle-2 text-error\"\n          >{{ custom.percent }}% Loss</v-list-item-subtitle\n        >\n        <template v-slot:append>\n          <v-list-item-subtitle class=\"text-subtitle-1 text-high-emphasis mr-3\">\n            ${{ custom.price }}</v-list-item-subtitle\n          >\n          <v-chip color=\"success\" label size=\"x-small\" v-if=\"custom.profit\">\n            <ChevronUpIcon size=\"17\" stroke-width=\"1.5\" />\n          </v-chip>\n          <v-chip color=\"error\" label size=\"x-small\" v-else>\n            <ChevronDownIcon size=\"17\" stroke-width=\"1.5\" />\n          </v-chip>\n        </template>\n      </v-list-item>\n      <v-divider></v-divider>\n    </template>\n  </v-list>\n</template>\n","<template>\n  <perfect-scrollbar style=\"max-height: 250px\">\n    <v-list>\n      <v-list-item v-for=\"(n, index) in 200\" :value=\"n\" :key=\"index\" color=\"primary\">\n        <v-list-item-title>Item {{ n }}</v-list-item-title>\n      </v-list-item>\n    </v-list>\n  </perfect-scrollbar>\n</template>\n","<script setup lang=\"ts\">\nimport { ref } from 'vue';\n// common components\nimport BaseBreadcrumb from '@/components/shared/BaseBreadcrumb.vue';\nimport UiParentCard from '@/components/shared/UiParentCard.vue';\nimport UiChildCard from '@/components/shared/UiChildCard.vue';\n\nimport SimpleList from '@/components/ui-components/list/SimpleList.vue';\nimport NestedList from '@/components/ui-components/list/NestedList.vue';\nimport FolderList from '@/components/ui-components/list/FolderList.vue';\nimport DisabledList from '@/components/ui-components/list/DisabledList.vue';\nimport SwitchList from '@/components/ui-components/list/SwitchList.vue';\nimport RoundedList from '@/components/ui-components/list/RoundedList.vue';\nimport CustomAlignList from '@/components/ui-components/list/CustomAlignList.vue';\nimport ScrollableList from '@/components/ui-components/list/ScrollableList.vue';\n\n// theme breadcrumb\nconst page = ref({ title: 'Lists' });\nconst breadcrumbs = ref([\n  {\n    text: 'Dashboard',\n    disabled: false,\n    href: '/',\n  },\n  {\n    text: 'Lists',\n    disabled: true,\n    href: '#',\n  },\n]);\n</script>\n\n<template>\n  <BaseBreadcrumb :title=\"page.title\" :breadcrumbs=\"breadcrumbs\"></BaseBreadcrumb>\n  <v-row>\n    <v-col cols=\"12\">\n      <UiParentCard title=\"List\">\n        <v-row>\n          <!-- Basic -->\n          <v-col cols=\"12\" lg=\"6\">\n            <UiChildCard title=\"Simple List\">\n              <SimpleList />\n            </UiChildCard>\n          </v-col>\n          <!-- Nested List -->\n          <v-col cols=\"12\" lg=\"6\">\n            <UiChildCard title=\"Nested List\">\n              <NestedList />\n            </UiChildCard>\n          </v-col>\n          <!-- Folder List -->\n          <v-col cols=\"12\" lg=\"6\">\n            <UiChildCard title=\"Folder List\">\n              <FolderList />\n            </UiChildCard>\n          </v-col>\n          <!-- DisabledList-->\n          <v-col cols=\"12\" lg=\"6\">\n            <UiChildCard title=\"Disabled List\">\n              <DisabledList />\n            </UiChildCard>\n          </v-col>\n          <!-- Radio Button List -->\n          <v-col cols=\"12\" lg=\"6\">\n            <UiChildCard title=\"Switch List\">\n              <SwitchList />\n            </UiChildCard>\n          </v-col>\n\n          <!-- Rounded List -->\n          <v-col cols=\"12\" lg=\"6\">\n            <UiChildCard title=\"Rounded List\">\n              <RoundedList />\n            </UiChildCard>\n          </v-col>\n          <!-- Custom Aligned List -->\n          <v-col cols=\"12\" lg=\"6\">\n            <UiChildCard title=\"Custom Aligned List\">\n              <CustomAlignList />\n            </UiChildCard>\n          </v-col>\n          <!-- Scrollable List -->\n          <v-col cols=\"12\" lg=\"6\">\n            <UiChildCard title=\"Scrollable List\">\n              <ScrollableList />\n            </UiChildCard>\n          </v-col>\n        </v-row>\n      </UiParentCard>\n    </v-col>\n  </v-row>\n</template>\n"],"version":3}