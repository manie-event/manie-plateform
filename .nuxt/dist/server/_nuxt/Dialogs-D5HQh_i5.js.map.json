v-col>\n\n          <v-col cols=\"12\" sm=\"12\" lg=\"4\" class=\"d-flex align-items-stretch\">\n            <UiChildCard title=\"Persistent\">\n              <DialogsPersistent />\n            </UiChildCard>\n          </v-col>\n          <v-col cols=\"12\" sm=\"12\" lg=\"4\" class=\"d-flex align-items-stretch\">\n            <UiChildCard title=\"Scrollable\">\n              <DialogsScrollable />\n            </UiChildCard>\n          </v-col>\n\n          <v-col cols=\"12\" sm=\"12\" lg=\"4\" class=\"d-flex align-items-stretch\">\n            <UiChildCard title=\"Nested Dialog\">\n              <DialogsNested />\n            </UiChildCard>\n          </v-col>\n        </v-row>\n      </UiParentCard>\n    </v-col>\n  </v-row>\n</template>\n"],"version":3}