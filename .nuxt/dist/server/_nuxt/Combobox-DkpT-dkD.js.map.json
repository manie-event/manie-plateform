-model=\"values\"\n          :items=\"items\"\n          density=\"comfortable\"\n          label=\"Comfortable\"\n          hide-details\n        ></v-combobox>\n      </v-col>\n      <v-col cols=\"12\">\n        <v-combobox\n          v-model=\"values\"\n          :items=\"items\"\n          density=\"compact\"\n          label=\"Compact\"\n          hide-details\n        ></v-combobox>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n","<script setup lang=\"ts\">\nimport { ref } from 'vue';\nconst select = ref(['Vuetify', 'Programming']);\nconst items = ref(['Programming', 'Design', 'Vue', 'Vuetify']);\n</script>\n<template>\n  <!-- ----------------------------------------------------------------------------- -->\n  <!-- Multiple Options -->\n  <!-- ----------------------------------------------------------------------------- -->\n  <div>\n    <v-row>\n      <v-col cols=\"12\">\n        <v-combobox\n          v-model=\"select\"\n          :items=\"items\"\n          hide-details\n          label=\"Select a favorite activity or create a new one\"\n          multiple\n        ></v-combobox>\n      </v-col>\n      <v-col cols=\"12\">\n        <v-combobox\n          v-model=\"select\"\n          :items=\"items\"\n          hide-details\n          label=\"I use chips\"\n          multiple\n          chips\n        ></v-combobox>\n      </v-col>\n      <v-col cols=\"12\">\n        <v-combobox\n          v-model=\"select\"\n          :items=\"items\"\n          hide-details\n          label=\"I use a scoped slot\"\n          multiple\n        >\n          <template v-slot:selection=\"data\">\n            <v-chip\n              :key=\"JSON.stringify(data.item)\"\n              v-bind=\"data.attrs\"\n              :model-value=\"data.selected\"\n              :disabled=\"data.disabled\"\n              size=\"small\"\n              @click:close=\"data.parent.selectItem(data.item)\"\n            >\n              <template v-slot:prepend>\n                <v-avatar class=\"bg-primary text-uppercase\" start>{{\n                  data.item.title.slice(0, 1)\n                }}</v-avatar>\n              </template>\n              {{ data.item.title }}\n            </v-chip>\n          </template>\n        </v-combobox>\n      </v-col>\n      <v-col cols=\"12\">\n        <v-combobox\n          v-model=\"select\"\n          hide-details\n          label=\"I'm readonly\"\n          chips\n          multiple\n          readonly\n        ></v-combobox>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n","<script setup lang=\"ts\">\nimport { ref } from 'vue';\n// common components\nimport BaseBreadcrumb from '@/components/shared/BaseBreadcrumb.vue';\nimport UiParentCard from '@/components/shared/UiParentCard.vue';\nimport UiChildCard from '@/components/shared/UiChildCard.vue';\nimport ComboboxDense from '@/components/forms/form-elements/combobox/ComboboxDense.vue';\nimport ComboboxMultiple from '@/components/forms/form-elements/combobox/ComboboxMultiple.vue';\n\n// theme breadcrumb\nconst page = ref({ title: 'Combobox' });\nconst breadcrumbs = ref([\n  {\n    text: 'Dashboard',\n    disabled: false,\n    href: '/',\n  },\n  {\n    text: 'Combobox',\n    disabled: true,\n    href: '#',\n  },\n]);\n</script>\n<template>\n  <BaseBreadcrumb :title=\"page.title\" :breadcrumbs=\"breadcrumbs\"></BaseBreadcrumb>\n  <v-row>\n    <v-col cols=\"12\">\n      <UiParentCard title=\"Combobox\">\n        <v-row>\n          <!-- Density Combobox -->\n          <v-col cols=\"12\" sm=\"12\">\n            <UiChildCard title=\"Density\">\n              <ComboboxDense />\n            </UiChildCard>\n          </v-col>\n          <!-- Multiple Combobox -->\n          <v-col cols=\"12\" sm=\"12\">\n            <UiChildCard title=\"Multiple\">\n              <ComboboxMultiple />\n            </UiChildCard>\n          </v-col>\n        </v-row>\n      </UiParentCard>\n    </v-col>\n  </v-row>\n</template>\n"],"version":3}